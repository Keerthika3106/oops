package sumofnumbers;
public class Sum {
	
	void add(int a,int b)
	{
		int sum=0;
		for(int i=a;i<=b;i++) {
			sum=sum+i;
		}
		System.out.println("The Sum of "+a+ " to "+b+ " is " +sum);
	}
	public static void main(String[] args) {
		
		Sum num =new Sum();
		num.add(1, 10);
		num.add(1,50);
		num.add(50,100);
		num.add(1, 100);
		num.add(1, 1);
		
	}
}


