package car;
class Car {
    String model;
    String color;

    // Constructor
    Car(String model, String color) {
        this.model = model;
        this.color = color;
    }

    // Method to start the car
    void start() {
        System.out.println(color+" "+model+" Car Started");
    }

    // Method to stop the car
    void stop() {
        System.out.println(color+" "+model +" Car Stopped");
    }
}

public class Main {
    public static void main(String[] args) {
        // Create car object
        Car car1 = new Car("Tesla ", "Red");
        Car car2 = new Car("BMW", "Blue");
        Car car3 = new Car("","Yellow");
        // Call methods
        car1.start();
        car2.start();
        car3.start();
        car3.stop();
    }
}



