package overloading;

public class A {

	int a;
	String name,dept;
	void set(int a) {
		this.a=a;
		System.out.println("The value of a is "+a);
	}
	
	void set(int a,String name) {
		this.a=a;
		this.name=name;
		System.out.println("The value of a is "+a+" and name is "+name);
	}
	
	void set(String name,String dept) {
		this.name=name;
		this.dept=dept;
		System.out.println("The name is "+name+" and dept is "+dept);
	}
	
	public static void main(String[] args) {
		
		A ob =new A();
		ob.set(4,"divya");	
		ob.set("divya","CSE");
		ob.set(5);
	}

}
